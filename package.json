{
  "name": "crypto-tracker",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "7.9.0",
    "npm": "4.2.0"
  },
  "engineStrict": true,
  "dependencies": {
    "amazon-cognito-identity-js": "2.0.1",
    "contentful": "4.6.1",
    "gh-pages": "1.0.0",
    "jquery": "3.2.1",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "moment-timezone": "0.5.13",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "7.0.0",
    "react": "16.0.0",
    "react-datepicker": "0.64.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.1.3",
    "react-loading": "0.1.4",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-reset": "0.3.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "prettier": "1.11.1",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "dev": "npm run start | npm run watch-css",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
