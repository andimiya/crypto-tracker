{
  "name": "crypto-tracker",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^8.0.0",
    "npm": "5.0"
  },
  "engineStrict": true,
  "dependencies": {
    "amazon-cognito-identity-js": "2.0.13",
    "audit": "0.0.6",
    "contentful": "4.6.1",
    "jquery": "3.3.1",
    "marked": "0.4.0",
    "moment": "2.22.2",
    "moment-timezone": "0.5.21",
    "node-sass-chokidar": "1.3.0",
    "normalize.css": "7.0.0",
    "react": "16.0.0",
    "react-datepicker": "1.5.0",
    "react-dom": "16.0.0",
    "react-loading": "2.0.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-reset": "0.3.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "prettier": "1.13.7",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy":
      "npm run build | aws s3 sync build/ s3://crypto-track.com --delete --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch",
    "start": "react-scripts start",
    "dev": "npm run start | npm run watch-css",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
